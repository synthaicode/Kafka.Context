INSERT INTO `t_22_contains_startswith_endswith` SELECT ('abcdef' LIKE '%cd%' ESCAPE '^') AS `LikeContains`, ('abcdef' LIKE 'ab%' ESCAPE '^') AS `LikeStartsWith`, ('abcdef' LIKE '%ef' ESCAPE '^') AS `LikeEndsWith`, ((POSITION('cd' IN 'abcdef') - 1) >= 0) AS `PositionContains`, (SUBSTRING('abcdef', 1, 2) = 'ab') AS `SubstringStartsWith`, (SUBSTRING('abcdef', 5, 2) = 'ef') AS `SubstringEndsWith` FROM `probe` AS t0;